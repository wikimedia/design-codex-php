<div class="cdx-text-input{{#hasError}} cdx-text-input--status-error{{/hasError}}{{#hasStartIcon}} cdx-text-input--has-start-icon{{/hasStartIcon}}{{#hasEndIcon}} cdx-text-input--has-end-icon{{/hasEndIcon}}{{#additionalWrapperClasses}} {{additionalWrapperClasses}}{{/additionalWrapperClasses}}" {{#wrapperAttributes}}{{{ wrapperAttributes }}}{{/wrapperAttributes}}>
  <input class="cdx-text-input__input{{#additionalInputClasses}} {{additionalInputClasses}}{{/additionalInputClasses}}" type="{{ type }}"{{#inputId}} id="{{ inputId }}"{{/inputId}} name="{{ name }}" value="{{ value }}" {{#placeholder }}placeholder="{{ placeholder }}"{{/placeholder }}{{#isDisabled}} disabled{{/isDisabled}}{{#inputAttributes}}{{{ inputAttributes }}}{{/inputAttributes}}/>
  {{#hasStartIcon}}
    <span class="cdx-text-input__icon cdx-text-input__start-icon {{ startIconClass }}"></span>
  {{/hasStartIcon}}
  {{#hasEndIcon}}
    <span class="cdx-text-input__icon cdx-text-input__end-icon {{ endIconClass }}"></span>
  {{/hasEndIcon}}
</div>
