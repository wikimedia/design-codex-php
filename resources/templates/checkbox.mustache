<div class="cdx-checkbox{{!
  }}{{#isInline}} cdx-checkbox--inline{{/isInline}}{{#additionalWrapperClasses}} {{additionalWrapperClasses}}{{/additionalWrapperClasses}}"{{!
  }}{{#wrapperId}} id="{{ wrapperId }}"{{/wrapperId}}{{!
  }}{{#wrapperAttributes}}{{{ wrapperAttributes }}}{{/wrapperAttributes}}>
  <input class="cdx-checkbox__input{{#additionalInputClasses}} {{additionalInputClasses}}{{/additionalInputClasses}}" type="checkbox"{{#inputId}} id="{{ inputId }}"{{/inputId}} name="{{ name }}" value="{{ value }}"{{!
    }}{{#isChecked}} checked{{/isChecked}}{{!
    }}{{#isDisabled}} disabled{{/isDisabled}}{{!
    }}{{#ariaDescribedby}} aria-describedby="{{ ariaDescribedby }}"{{/ariaDescribedby}}{{!
    }}{{#inputAttributes}}{{{ inputAttributes }}}{{/inputAttributes}}>
  <span class="cdx-checkbox__icon"></span>
  {{#label}}
    {{> label }}
  {{/label}}
</div>
